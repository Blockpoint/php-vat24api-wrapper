{
    "name": "blockpoint/vat24api-php",
    "description": "API wrapper for Vat24API",
    "keywords": [
        "Blockpoint",
        "vat24api-php"
    ],
    "homepage": "https://github.com/blockpoint/vat24api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Blockpoint",
            "email": "info@blockpoint.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3.2",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Blockpoint\\Vat24Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blockpoint\\Vat24Api\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {

            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
